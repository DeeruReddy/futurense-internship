    "#python program to print factorial of a number\n",
    "def factorial(x): \n",
    "       \n",
    "    if x == 0: \n",
    "        return 1\n",
    "    else:\n",
    "        return x * factorial(x-1) \n",
    "a = int(input(\"Enter a number: \")); \n",
    "print(\"Factorial of\", a , \"is\", \n",
    "factorial(a)) "
   ]
